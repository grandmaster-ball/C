#include<iostream>
#include<iomanip>

using namespace std;
int main()
{
    int a,b,d[100];
    cin>>a;
    b=a;
    int n=0;
    while(a){
        a=a/10;
        n++;
    }
    cout<<n<<endl;
    for(int i=0;i<n;i++)
    {
        d[i]=b%10;
        b=b/10;
    }
    for(int i=n-1;i>=0;i--) {
        if(i!=0) cout<<d[i]<<' ';
        else cout<<d[i]<<endl;
    }
    for(int i=0;i<n;i++) cout<<d[i];
}
